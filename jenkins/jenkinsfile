    agent any
    stages {
        
         stage('Send Email'){
           steps
          {
          emailext body: '${SCRIPT, template="groovy-html.template"}',
          recipientProviders: [[$class: 'DevelopersRecipientProvider'], [$class: 'RequesterRecipientProvider']],
          subject: 'Build Successful ${JOB_NAME}',
          to: 'naveenkumarreddyp9@gmail.com',
         mimeType: 'text/html'

         }
               
        }


}
    post {
        always
    }
{
        always {
            cleanWs()
        }

  }
 
}


